cp notebooks/* manual 
cd manual
make clean
jupyter-nbconvert --to rst --TemplateExporter.exclude_input=True  *.ipynb
make markdown
make html
make latexpdf
make epub
cp _build/latex/*.pdf .. 
cp _build/epub/*.epub .. 
cp -rfp _build/markdown/*  ../docs
cp -rfp _build/html  ../docs
cd ../docs
